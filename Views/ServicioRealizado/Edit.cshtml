@model amazon.Models.ServicioRealizado

@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>

<form asp-action="Edit">
    <input type="hidden" asp-for="Id" />
    <div class="form-group">
        <label asp-for="ServicioId" class="control-label"></label>
        <select asp-for="ServicioId" class="form-control" asp-items="@ViewBag.ServicioId"></select>
        <span asp-validation-for="ServicioId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="UsuarioId" class="control-label"></label>
        <select asp-for="UsuarioId" class="form-control" asp-items="@ViewBag.UsuarioId"></select>
        <span asp-validation-for="UsuarioId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Precio" class="control-label"></label>
        <input asp-for="Precio" class="form-control" />
        <span asp-validation-for="Precio" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Fecha" class="control-label"></label>
        <input asp-for="Fecha" class="form-control" />
        <span asp-validation-for="Fecha" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
